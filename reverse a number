a = int(input("please enter the number: "))
temp = a 
reverse = 0 
while a > 0:
    remainder = a % 10 
    reverse = (reverse * 10) + remainder
    a // 10 
    
print("The Given number is {} and Reverse is {}".format(temp, reverse))


(or)

def reversed(x):
  y=0
  while x > 0:
    digit = x % 10
    y = y * 10 + digit
    x = x // 10 
  return y
print(reversed(456)) 
